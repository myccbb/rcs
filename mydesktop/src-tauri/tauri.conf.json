{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:51420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "mydesktop",
    "version": "0.0.0"
  },
  "tauri": {
    "cli": {
      "description": "",
      "longDescription": "",
      "beforeHelp": "",
      "afterHelp": "",
      "args": [
        {
          "name": "app",
          "takesValue": true,
          "possibleValues": [
            "daily",
            "bookmark",
            "collection"
          ]
        }
      ]
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.mybase.mydesktop",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "mydesktop",
        "width": 800,
        "height": 600
      }
    ]
  }
}
